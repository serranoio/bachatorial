{
  "name": "rspress-doc-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rspress build",
    "build:export": "bun run scripts/build-export-html.ts",
    "dev": "rspress dev",
    "preview": "rspress preview",
    "preview:stories": "bun run scripts/serve-stories.ts",
    "export:stories": "node scripts/export-stories-mp4.mjs",
    "retest": "rm -rf exports && mkdir exports && node scripts/export-stories-mp4.mjs"
  },
  "dependencies": {
    "rspress": "^1.40.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "playwright": "^1.57.0"
  }
}
