{
  "name": "rspress-doc-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rspress build",
    "build:export": "bun run scripts/build-export-html.ts",
    "dev": "rspress dev",
    "preview": "rspress preview",
    "preview:stories": "bun run scripts/serve-stories.ts",
    "export:stories": "bun scripts/export-stories-mp4.mjs",
    "retest": "bun run build:export && rm -rf exports && mkdir exports && bun scripts/export-stories-mp4.mjs",
    "move-to-photos": "bash scripts/move-exports-to-photos.sh"
  },
  "dependencies": {
    "rspress": "^1.40.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "playwright": "^1.57.0"
  }
}
